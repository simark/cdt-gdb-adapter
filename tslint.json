{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "arrow-parens": [true, "ban-single-arg-parens"],
    "interface-name": false,
    "no-empty-interface": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single",
      "avoid-template",
      "avoid-escape"
    ]
  }
}
